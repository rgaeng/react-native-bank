{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/jsx-filename-extension": [0, {
      "extensions": [".jsx"]
    }],
    "import/extensions": ["error", "never", {
      "svg": "always",
      "jpg": "always",
      "png": "always"
    }],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "import/no-unresolved": "off",
    "no-shadow": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": "off",
    "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }],
    "jsx-a11y/media-has-caption": "off"
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "prettier"
  ]
}
